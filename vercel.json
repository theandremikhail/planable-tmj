{
  "buildCommand": "vite build",
  "outputDirectory": "dist",
  "framework": "vite",
  "devCommand": "vite --port $PORT",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" }
  ],
  "crons": [
    {
      "path": "/api/cron/publish-scheduled",
      "schedule": "0 9 * * *"
    }
  ]
}
